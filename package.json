{
  "name": "@signumjs/did-resolver",
  "version": "0.1.0",
  "description": "W3C DID Resolver for Signum Network - Resolve decentralized identifiers for accounts, transactions, aliases, contracts, and tokens on the Signum blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "signum",
    "did",
    "decentralized-identifier",
    "w3c",
    "blockchain",
    "identity",
    "resolver",
    "verifiable-credentials"
  ],
  "author": "ohager",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/signum-network/signum-did-resolver"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@signumjs/core": "^2.0.8",
    "@signumjs/http": "^2.0.8",
    "@signumjs/standards": "^2.0.8",
    "@signumjs/util": "^2.0.8",
    "hono": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vercel/node": "^3.0.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.10",
    "pinst": "^3.0.0",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.0",
    "vercel": "^33.0.0",
    "vitest": "^1.2.0"
  }
}
